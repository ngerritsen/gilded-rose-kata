{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "noImplicitAny": false,
    "jsx": "react",
    "jsxImportSource": "@emotion/react",
    "sourceMap": false,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["app/*"]
    }
  },
  "exclude": ["node_modules"]
}
